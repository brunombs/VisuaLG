algoritmo "ContarDeDezAtéZero"
var
   contador: inteiro
inicio
      contador <- 10
      Enquanto (contador >= 0) faca
               Escreval (contador)
               contador <- contador -1
      FimEnquanto
      Escreval ("Terminei a contagem regressiva")
fimalgoritmo

